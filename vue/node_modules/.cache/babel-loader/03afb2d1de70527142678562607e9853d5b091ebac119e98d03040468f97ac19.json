{"ast":null,"code":"export default {\n  data() {\n    return {\n      carouselData: [require('@/assets/imgs/da4a9058921d7a2ea96e625c8c940e87.png')],\n      recommend: {},\n      rightData: []\n    };\n  },\n  mounted() {\n    this.loadRecommend();\n    this.loadRightData();\n  },\n  methods: {\n    loadRecommend() {\n      this.$request.get('/course/getRecommend').then(res => {\n        if (res.code === '200') {\n          this.recommend = res.data;\n          this.loadRightData(); // 在获取推荐课程后再加载其他课程\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    },\n    playVideo() {\n      const video = this.$refs.introVideo;\n      video.style.display = 'block'; // 显示视频\n      video.style.opacity = '1'; // 渐变到可见\n      this.$refs.recommendImage.style.opacity = '0';\n      video.play(); // 播放视频\n\n      // 设置定时器，5秒后暂停并隐藏视频\n      setTimeout(() => {\n        video.pause();\n        video.currentTime = 0; // 重置到开始\n        video.style.opacity = '0'; // 渐变到不可见\n        setTimeout(() => {\n          video.style.display = 'none'; // 隐藏视频\n        }, 500); // 等待0.5秒再隐藏\n      }, 5000);\n    },\n    pauseVideo() {\n      const video = this.$refs.introVideo;\n      video.pause(); // 暂停视频\n      video.currentTime = 0; // 重置到开始\n      video.style.opacity = '0'; // 渐变到不可见\n      this.$refs.recommendImage.style.opacity = '1'; // 确保图片可见\n      this.$refs.recommendImage.style.display = 'block'; // 确保图片显示\n      video.onended = () => {\n        this.handleVideoEnd(); // 调用处理结束的方法\n      };\n\n      setTimeout(() => {\n        video.style.display = 'none'; // 隐藏视频\n      }, 10000); // 等待0.5秒再隐藏\n    },\n\n    handleVideoEnd() {\n      const video = this.$refs.introVideo;\n      video.pause(); // 确保视频暂停\n      video.currentTime = 0; // 重置到开始\n      video.style.opacity = '0'; // 渐变到不可见\n      this.$refs.recommendImage.style.opacity = '1'; // 立即显示图片\n\n      setTimeout(() => {\n        video.style.display = 'none'; // 隐藏视频\n      }, 500); // 等待0.5秒再隐藏\n    },\n\n    navTo(id) {\n      if (id) {\n        this.playVideo(); // 播放视频\n        const video = this.$refs.introVideo;\n        video.onended = () => {\n          location.href = '/front/courseDetail?id=' + id; // 跳转到课程详情页\n        };\n      } else {\n        this.$message.error('课程信息缺失'); // 提示课程信息缺失\n      }\n    },\n\n    loadRightData() {\n      this.$request.get('/course/selectTop8').then(res => {\n        if (res.code === '200') {\n          // 将推荐课程从数据中排除\n          const allCourses = res.data;\n          const filteredCourses = allCourses.filter(course => course.id !== this.recommend.id);\n          this.rightData = filteredCourses.slice(0, 8); // 选择最多8个课程\n        } else {\n          this.$message.error(res.msg);\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","carouselData","require","recommend","rightData","mounted","loadRecommend","loadRightData","methods","$request","get","then","res","code","$message","error","msg","playVideo","video","$refs","introVideo","style","display","opacity","recommendImage","play","setTimeout","pause","currentTime","pauseVideo","onended","handleVideoEnd","navTo","id","location","href","allCourses","filteredCourses","filter","course","slice"],"sources":["src/views/front/Home.vue"],"sourcesContent":["<!--<template>-->\r\n<!--  <div class=\"main-content\">-->\r\n<!--    <div style=\"display: flex\">-->\r\n<!--      <div style=\"flex: 12%; height: 350px; background-color: #0c0c0c\"></div>-->\r\n<!--      <div style=\"flex: 76%\">-->\r\n<!--        <el-carousel height=\"350px\">-->\r\n<!--          <el-carousel-item v-for=\"(item, index) in carouselData\" :key=\"index\">-->\r\n<!--            <img :src=\"item\" alt=\"\" style=\"width: 100%; height: 350px\">-->\r\n<!--          </el-carousel-item>-->\r\n<!--        </el-carousel>-->\r\n<!--      </div>-->\r\n<!--      <div style=\"flex: 12%; height: 350px; background-color: #0c0c0c\"></div>-->\r\n<!--    </div>-->\r\n\r\n<!--    <div style=\"width: 76%; margin: 50px auto; height: auto;\">-->\r\n<!--      <div style=\"display: flex; align-items: center; margin-bottom: 20px;\">-->\r\n<!--        <span style=\"font-size: 24px; color: #333333;\">在线课程</span>-->\r\n<!--      </div>-->\r\n\r\n<!--      <div style=\"display: flex; margin-top: 20px; flex-wrap: wrap;\">-->\r\n<!--        <div style=\"flex: 1; margin-right: 10px;\">-->\r\n<!--          <div class=\"course-card\" @click=\"navTo(recommend.id)\">-->\r\n<!--            <img :src=\"recommend.img\" alt=\"\" class=\"course-image\">-->\r\n<!--            <div class=\"course-title\">{{ recommend.name }}</div>-->\r\n<!--          </div>-->\r\n<!--        </div>-->\r\n<!--        <div style=\"flex: 2; display: flex; flex-wrap: wrap;\">-->\r\n<!--          <el-col :span=\"6\" v-for=\"item in rightData\" :key=\"item.id\" style=\"margin-bottom: 35px;\">-->\r\n<!--            <div class=\"course-card\" @click=\"navTo(item.id)\">-->\r\n<!--              <img :src=\"item.img\" alt=\"\" class=\"course-image\">-->\r\n<!--              <div class=\"course-title\">{{ item.name }}</div>-->\r\n<!--            </div>-->\r\n<!--          </el-col>-->\r\n<!--        </div>-->\r\n<!--      </div>-->\r\n<!--    </div>-->\r\n<!--  </div>-->\r\n<!--</template>-->\r\n\r\n<!--<script>-->\r\n<!--export default {-->\r\n<!--  data() {-->\r\n<!--    return {-->\r\n<!--      carouselData: [-->\r\n<!--        require('@/assets/imgs/da4a9058921d7a2ea96e625c8c940e87.png'),-->\r\n<!--      ],-->\r\n<!--      recommend: {},-->\r\n<!--      rightData: [],-->\r\n<!--    }-->\r\n<!--  },-->\r\n\r\n<!--  mounted() {-->\r\n<!--    this.loadRecommend();-->\r\n<!--    this.loadRightData();-->\r\n<!--  },-->\r\n\r\n<!--  methods: {-->\r\n<!--    loadRecommend() {-->\r\n<!--      this.$request.get('/course/getRecommend').then(res => {-->\r\n<!--        if (res.code === '200') {-->\r\n<!--          this.recommend = res.data;-->\r\n<!--        } else {-->\r\n<!--          this.$message.error(res.msg);-->\r\n<!--     